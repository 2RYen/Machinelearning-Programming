   std_true = np.sqrt(np.sum((y_true - np.mean(y_true)) ** 2))\n","    std_pred = np.sqrt(np.sum((y_pred - np.mean(y_pred)) ** 2))\n","    return cov / std_true / std_pred"],"metadata":{"id":"Z9h3fOShfONc","executionInfo":{"status":"ok","timestamp":1749739796644,"user_tz":-540,"elapsed":7,"user":{"displayName":"김민주","userId":"13715885916761677384"}}},"execution_count":3,"outputs":[]},{"cell_type":"markdown","source":["# 3. Comparison between Scikit-learn and Numpy-based Calculation"],"metadata":{"id":"GwKISn3egl4Z"}},{"cell_type":"code","source":["print(\"MAE(sklearn):\", mean_absolute_error(y_true, y_pred))\n","print(\"MAE(numpy):\", np_mae(y_true, y_pred))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"eIGKYDJXgOHc","executionInfo":{"status":"ok","timestamp":1749741056176,"user_tz":-540,"elapsed":25,"user":{"displayName":"김민주","userId":"13715885916761677384"}},"outputId":"dd9269e1-3448-41d5-be81-8d662162a89c"},"execution_count":4,"outputs":[{"output_type":"stream","name":"stdout","text":["MAE(sklearn): 0.5\n","MAE(numpy): 0.5\n"]}]},{"cell_type":"code","source":["print(\"MSE(sklearn):\", mean_squared_error(y_true, y_pred))\n","print(\"MSE(numpy):\", np_mse(y_true, y_pred))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"bqrBN1H1gVTH","executionInfo":{"status":"ok","timestamp":1749741214144,"user_tz":-540,"elapsed":21,"user":{"displayName":"김민주","userId":"13715885916761677384"}},"outputId":"85ee3c04-669e-4d00-9571-dde6a92e19a9"},"execution_count":5,"outputs":[{"output_type":"stream","name":"stdout","text":["MSE(sklearn): 0.375\n","MSE(numpy): 0.375\n"]}]},{"cell_type":"code","source":["print(\"RMSE(sklearn):\", root_mean_squared_error(y_true, y_pred))\n","print(\"RMSE(numpy):\", np_rmse(y_true, y_pred))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"OWV3oA--gX8c","executionInfo":{"status":"ok","timestamp":1749741272326,"user_tz":-540,"elapsed":13,"user":{"displayName":"김민주","userId":"13715885916761677384"}}